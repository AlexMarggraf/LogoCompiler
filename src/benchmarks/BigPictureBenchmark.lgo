to nlBoard :s
  pu fd 250*:s lt 90 fd 8*250*:s rt 90 pd
end

to nlPiece :n :s
  pu fd 7*:s lt 90 fd :n*7*:s rt 90 pd
  #pu fd 100 lt 90 fd :n*100 rt 90 pd
end

to place :x :y :dx :dy :s
  setxy ((:x - 1)*250 + :dx)*:s ((:y - 1)*250 + :dy)*:s
end

to newNumber :n :s
  if :n = 0 [ # Horizontal
    pu bk 250*:s rt 90 fd 250*:s lt 90 pd
    ]
end
