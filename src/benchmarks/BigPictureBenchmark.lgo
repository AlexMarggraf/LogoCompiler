to redrawSquare :x :y :s

  if :x - 1 > 7 [
    STOP
    ]

  if :y - 1 > 7 [
    STOP
    ]

  if :x - 1 < 0 [
    STOP
    ]

  if :y - 1< 0 [
    STOP
    ]

  if (:x - 1) % 2 = 0 [
    if (:y - 1) % 2 = 0 [
      place :x :y 0 0 :s
      blackSquare :s
      ]
    if (:y - 1) % 2 = 1 [
      place :x :y 0 0 :s
      whiteSquare :s
      ]
  ]

  if (:x - 1) % 2 = 1 [
    if (:y - 1) % 2 = 0 [
      place :x :y 0 0 :s
      whiteSquare :s
      ]
    if (:y - 1) % 2 = 1 [
      place :x :y 0 0 :s
      blackSquare :s
      ]
  ]
end

